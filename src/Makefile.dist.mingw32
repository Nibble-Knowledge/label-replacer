CC=i686-pc-mingw32-gcc
DISTFLAGS=-m32 -Os -s -static -march=native -fomit-frame-pointer -pipe -Wdisabled-optimization
CFLAGS=$(DISTFLAGS) $(EXTRACFLAGS) -c
LDFLAGS=$(DISTFLAGS) $(EXTRALDFLAGS)
SRC+=getline.c
OBJ=$(SRC:.c=.o)

all: $(SRC) $(EXE)

$(EXE): $(OBJ)
	$(CC) $(OBJ) -o $@ $(LDFLAGS)

%.o: %.c
	$(CC) $(CFLAGS) $< -o $@
